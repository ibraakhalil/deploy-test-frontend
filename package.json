{
  "name": "ihadis-frontend-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:all": "concurrently --names \"Gql,Next\" --prefix-colors \"blue.bold,magenta.bold\" \"npm run watch\" \"npm run dev\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "cd .. && husky install frontend/.husky",
    "commit": "git-cz",
    "generate-types": "openapi-typescript http://localhost:3000/api/docs -o src/types/api.ts"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.11.5",
    "@apollo/experimental-nextjs-app-support": "^0.11.2",
    "@radix-ui/react-accordion": "^1.2.0",
